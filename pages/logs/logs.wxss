.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
}
.times{
  font-size: 36rpx;
  padding: 40rpx;
  color: rgb(51,51,51);
  background: rgb(243,243,243);
  box-sizing: border-box;
}
.logsList{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 40rpx;
  border-bottom: 20rpx solid rgb(243,243,243);
}
.logsList .logName{
  font-size: 36rpx;
  color: rgb(51,51,51);
}
.logsList .logTime,.logsList .success{
  font-size: 32rpx;
  color: rgb(102,102,102);
}
.logTime .openDay{
  margin-right: 30rpx;
  margin-top: 6rpx;
  font-family: 'PingFang-SC-Medium';
}